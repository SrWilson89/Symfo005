{% extends 'base.html.twig' %}

{% block title %}Hilo{% endblock %}

{% block body %}
    <h1>Hilo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ hilo.id }}</td>
            </tr>
            <tr>
                <th>DateAdd</th>
                <td>{{ hilo.dateAdd ?
 hilo.dateAdd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateMod</th>
                <td>{{ hilo.dateMod ?
 hilo.dateMod|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Notas</th>
                <td>{{ hilo.notas }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_list', {'entity': 'hilo'}) }}" class="btn btn-secondary">Volver a la lista</a>

    <a href="{{ path('app_hilo_edit', {'id': hilo.id}) }}">edit</a>

    {{ include('hilo/_delete_form.html.twig') }}
{% endblock %}